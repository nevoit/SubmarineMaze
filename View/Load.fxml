<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="170.0" prefWidth="369.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoadController">
   <children>
      <Label alignment="TOP_CENTER" layoutY="25.0" prefHeight="34.0" prefWidth="369.0" text="Please type the filename of the Maze you wish to load:" />
      <TextField fx:id="pathWayText" layoutX="63.0" layoutY="68.0" prefHeight="34.0" prefWidth="244.0" text="" />
      <Button fx:id="load_BTN" onAction="#load" layoutX="88.0" layoutY="116.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="74.0" text="Load" />
      <Button fx:id="cancel_BTN" onAction="#cancel" layoutX="203.0" layoutY="116.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="74.0" text="Cancel" />
   </children>
</AnchorPane>
